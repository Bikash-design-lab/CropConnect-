								"exec": [
									"pm.test(\"Response status code is 200\", function () {\r",
									"    pm.expect(pm.response.code).to.eql(200);\r",
									"});\r",
									"\r",
									"\r",
									"pm.test(\"Response time is less than 200ms\", function () {\r",
									"    pm.expect(pm.response.responseTime).to.be.below(200);\r",
									"});\r",
									"\r",
									"\r",
									"pm.test(\"Response schema matches the expected structure\", function () {\r",
									"    const responseData = pm.response.json();\r",
									"    \r",
									"    pm.expect(responseData).to.be.an('object');\r",
									"    pm.expect(responseData).to.have.all.keys('weekly_forecast');\r",
									"\r",
									"    pm.expect(responseData.weekly_forecast).to.be.an('array');\r",
									"    responseData.weekly_forecast.forEach(function(forecast) {\r",
									"        pm.expect(forecast).to.be.an('object');\r",
									"        pm.expect(forecast).to.have.all.keys('date', 'high', 'low');\r",
									"        pm.expect(forecast.date).to.be.a('string').that.is.not.empty;\r",
									"        pm.expect(forecast.high).to.be.a('number');\r",
									"        pm.expect(forecast.low).to.be.a('number');\r",
									"    });\r",
									"});\r",
									"\r",
									"\r",
									"pm.test(\"All fields in the response are of the expected data types\", function () {\r",
									"    const responseData = pm.response.json();\r",
									"    \r",
									"    pm.expect(responseData).to.be.an('object');\r",
									"    \r",
									"    // Check for expected data types\r",
									"    pm.expect(responseData).to.have.property('field1').that.is.a('string');\r",
									"    pm.expect(responseData).to.have.property('field2').that.is.a('number');\r",
									"    pm.expect(responseData).to.have.property('field3').that.is.a('boolean');\r",
									"    pm.expect(responseData).to.have.property('field4').that.is.a('array');\r",
									"    pm.expect(responseData).to.have.property('field5').that.is.an('object');\r",
									"});\r",
									"\r",
									"\r",
									"pm.test(\"Response does not contain any unexpected fields\", function () {\r",
									"    const responseData = pm.response.json();\r",
									"    \r",
									"    pm.expect(responseData).to.be.an('object');\r",
									"    const expectedFields = ['field1', 'field2', 'field3']; // Replace with actual expected fields\r",
									"    const unexpectedFields = Object.keys(responseData).filter(field => !expectedFields.includes(field));\r",
									"    \r",
									"    pm.expect(unexpectedFields).to.have.lengthOf(0, \"Response contains unexpected fields: \" + unexpectedFields.join(\", \"));\r",
									"});\r",
									""
